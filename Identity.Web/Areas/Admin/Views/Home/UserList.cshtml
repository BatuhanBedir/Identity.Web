@model List<AdminModel.UserViewModel>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<h2>User Listesi</h2>
<table class="table table-striped-columns">
    <thead>
    <tr>
        <th>Id</th>
        <th>Kullanıcı Adı</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserName</td>
            <td>@item.Email</td>
        </tr>
    }  
    </tbody>
</table>